## Enostavna koda za Testiranje 

#include <LiquidCrystal_I2C.h>
#include <Arduino.h>

#define SCL 39;
#define SDA 42;

LiquidCrystal_I2C lcd1(32,16,2);  //to ti zna≈° zrihtat

void setup() {
  Serial.begin(115200);
  lcd1.backlight();
  lcd1.init();


}

void loop() {
  lcd1.clear();
  lcd1.setCursor(0, 0);
  lcd1.print("Left: ");
  lcd1.print(leftServo.read());
  lcd1.setCursor(0, 1);
  lcd1.print("Right: ");
  lcd1.print(rightServo.read());
  delay(50);
}
